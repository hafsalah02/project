generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Utilisateur {
  id        Int    @id @default(autoincrement())
  nom       String
  prenom    String
  numero    String @unique
  email     String @unique
  matricule String @unique
  password  String
